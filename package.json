{
    "name": "catalog-service",
    "version": "1.0.0",
    "description": "Catalog Service",
    "main": "index.js",
    "scripts": {
        "dev": "cross-env NODE_ENV=development nodemon src/server.ts",
        "build": "tsc",
        "format:check": "prettier . --check",
        "format:fix": "prettier . --write",
        "lint": "eslint .",
        "lint:fix": "eslint . --fix",
        "test": "jest --no-cache --coverage --all --runInBand --passWithNoTests",
        "prepare": "husky",
        "start": "ts-node src/index.ts",
        "migration:generate": "typeorm-ts-node-commonjs migration:generate",
        "migration:run": "typeorm-ts-node-commonjs migration:run",
        "migration:create": "typeorm-ts-node-commonjs migration:create"
    },
    "author": "Dev",
    "license": "ISC",
    "devDependencies": {
        "@eslint/js": "^9.39.0",
        "@types/config": "^3.3.5",
        "@types/cookie-parser": "^1.4.10",
        "@types/express": "^5.0.5",
        "@types/express-fileupload": "^1.5.1",
        "@types/jest": "^30.0.0",
        "@types/node": "^24.9.2",
        "@types/supertest": "^6.0.3",
        "@types/uuid": "^10.0.0",
        "cross-env": "^10.1.0",
        "eslint": "^9.39.0",
        "husky": "^9.1.7",
        "jest": "^30.2.0",
        "lint-staged": "^16.2.6",
        "nodemon": "^3.1.10",
        "prettier": "3.6.2",
        "supertest": "^7.1.4",
        "ts-jest": "^29.4.6",
        "ts-node": "^10.9.2",
        "typescript": "^5.9.3",
        "typescript-eslint": "^8.46.2"
    },
    "lint-staged": {
        "*.ts": [
            "npm run format:fix",
            "npm run lint:fix"
        ]
    },
    "dependencies": {
        "@aws-sdk/client-s3": "^3.983.0",
        "@types/http-errors": "^2.0.5",
        "config": "^4.2.0",
        "cookie-parser": "^1.4.7",
        "dotenv": "^17.2.3",
        "express": "^5.1.0",
        "express-fileupload": "^1.5.2",
        "express-jwt": "^8.5.1",
        "express-validator": "^7.3.1",
        "http-errors": "^2.0.1",
        "js-yaml": "^4.1.1",
        "jwks-rsa": "^3.2.2",
        "mongoose": "^9.1.5",
        "uuid": "^8.3.2",
        "winston": "^3.19.0"
    }
}
